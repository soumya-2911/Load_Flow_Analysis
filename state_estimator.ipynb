    "# --------------------------------------------------------------------------- #\n",
    "# 5 ── DEMONSTRATION SIMULATION (synthetic trajectory + EKF)\n",
    "#      ⇢ Replace the arrays with load_csv('busdata.csv') etc. in production\n",
    "# --------------------------------------------------------------------------- #\n",
    "if __name__ == \"__main__\":\n",
    "\n",
    "    dataset=[]\n",
    "    Angle=[]\n",
    "    Faulty_Bus=[]\n",
    "    Fault_Types=[]\n",
    "    Scale=[]\n",
    "    \n",
    "    # ── load IEEE-14 sample data (replace here) ─#\n",
    "    bus_matrix,line,shunt=load_power_system_data()\n",
    "   \n",
    "    bus = BusData(\n",
    "        No    = bus_matrix[:, 0].astype(int),\n",
    "        Type  = bus_matrix[:, 1].astype(int),\n",
    "        Vm    = bus_matrix[:, 2], #initial prediction\n",
    "        ang   = bus_matrix[:, 3], #initial prediction\n",
    "        Pd    = bus_matrix[:, 6],\n",
    "        Qd    = bus_matrix[:, 7],\n",
    "        Pg    = bus_matrix[:, 4],\n",
    "        Qg    = bus_matrix[:, 5],\n",
    "    )\n",
    "    Ybus=form_ybus(bus.nbus,line,shunt)\n",
    "    Vm, ang, iters, V = newton_raphson(bus, Ybus,basemva=100)\n",
    "    bus_matrix[:,2]=Vm\n",
    "    bus_matrix[:,3]=ang\n",
    "    \n",
    "    print_bus_table(bus,Vm,ang,\"Newton Raphson\",iters)\n",
    "    \n",
    "    # ── synthesise ground-truth trajectory ─#\n",
    "    sim_N = 3001 #Number of timestamp\n",
    "    # fault_types = ['normal', 'sag', 'swell', 'harmonic']\n",
    "    fault_types = ['normal', 'sag', 'swell']\n",
    "    fault=fault_types[0]\n",
    "    # Select random time window for fault duration (between 10 and 50 steps)\n",
    "    start = np.random.randint(0, sim_N - 50)\n",
    "    end = start + np.random.randint(10, 50)\n",
    "    \n",
    "    nstate = 2*bus.nbus-1\n",
    "    X_true = np.zeros((nstate, sim_N))\n",
    "    X_true[:,0] = np.r_[bus_matrix[1:,3]*np.pi/180, bus_matrix[:,2]]\n",
    "    \n",
    "    # small random walk\n",
    "    Dth = sqrtm(1e-5*np.eye(bus.nbus-1))\n",
    "    DV  = sqrtm(1e-5*np.eye(bus.nbus))\n",
    "    for k in range(1, sim_N):\n",
    "        X_true[bus.nbus-1:,k] = X_true[bus.nbus-1:,k-1] + DV @ (2*np.random.rand(bus.nbus)-1)\n",
    "        X_true[:bus.nbus-1,k] = X_true[:bus.nbus-1,k-1] + Dth @ (2*np.random.rand(bus.nbus-1)-1)\n",
    "\n",
    "\n",
    "\n",
    "    # ── build noisy measurements (with bad-data spikes)  #\n",
    "    ekf = PowerSystemEKF(bus_matrix, line, shunt)\n",
    "    Z = np.zeros((ekf.R.shape[0], sim_N))\n",
    "    for k in range(sim_N):\n",
    "        z_k,a = ekf.h(X_true[:,k])\n",
    "        \n",
    "    #----------------------------------------------------------------------------------------------------------------------------    \n",
    "        # introduce faults here needed for cascading with ML model\n",
    "        if k in range(start, end):\n",
    "            z_k, a, faulty_busses, fault_type,mag = introduce_faults(ekf, X_true, fault, bus, k)\n",
    "\n",
    "            dataset.append(z_k)\n",
    "            Angle.append(a)\n",
    "            Faulty_Bus.append(faulty_busses)\n",
    "            Fault_Types.append(fault_type)\n",
    "            Scale.append(mag)\n",
    "            # print(f\"Fault type: {fault_type}, Faulty bus: {faulty_busses}\")\n",
    "\n",
    "        \n",
    "     #---------------------------------------------------------------------------------------------------------------------------- \n",
    "        Z[:,k] = z_k + sqrtm(ekf.R) @ np.random.randn(len(z_k))\n",
    "\n",
    "\n",
    "    # ── run EKF online ---- #\n",
    "    x_hat = np.zeros_like(X_true)\n",
    "    P     = 1e-5*np.eye(nstate)\n",
    "    lam   = np.zeros_like(Z)\n",
    "    x_hat[:,0] = X_true[:,0]\n",
    "\n",
    "    for k in range(1, sim_N):\n",
    "        x_pred, P_pred = ekf.predict(x_hat[:,k-1], P)\n",
    "        x_hat[:,k], P, lam[:,k] = ekf.update(x_pred, P_pred, Z[:,k])\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "def convert_to_serializable(obj):\n",
    "    if isinstance(obj, np.ndarray):\n",
    "        return obj.tolist()\n",
    "    elif isinstance(obj, (np.float32, np.float64)):\n",
    "        return float(obj)\n",
    "    elif isinstance(obj, (np.int32, np.int64)):\n",
    "        return int(obj)\n",
    "    # Add more conversions if needed\n",
    "    return obj\n",
    "\n",
    "\n",
    "\n",
    "    # # ── basic performance report ------- #\n",
    "    # rms_ang2 = np.sqrt(np.mean((x_hat[1,:]-X_true[1,:])**2))\n",
    "    # print(f\"RMS angle error @ bus 2 = {rms_ang2:.5f} rad\")\n"
